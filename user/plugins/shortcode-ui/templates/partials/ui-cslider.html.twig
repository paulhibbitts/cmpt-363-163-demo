<figure class="cd-image-container">
    {{ matches[0][1] }}
    <span class="cd-image-label" data-type="original">{{ matches[1][1] }}</span>
    <div class="cd-resize-img">
        {{ matches[0][0] }}
        <span class="cd-image-label" data-type="modified">{{ matches[1][0] }}</span>
    </div>
    <span class="cd-handle"><i class="fa fa-fw fa-chevron-left"></i><i class="fa fa-fw fa-chevron-right"></i></span>
</figure>